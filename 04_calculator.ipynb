            "source": [
                "# 4. 함수(Function) 연습\n",
                "def add(a, b):\n",
                "    return a + b\n",
                "\n",
                "def subtract(a, b):\n",
                "    return a - b\n",
                "\n",
                "# 함수 사용해보기\n",
                "num1 = 10\n",
                "num2 = 5\n",
                "\n",
                "print(f\"{num1} + {num2} = {add(num1, num2)}\")\n",
                "print(f\"{num1} - {num2} = {subtract(num1, num2)}\")"
            ]
        }
    ],
