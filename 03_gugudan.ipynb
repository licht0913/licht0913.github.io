            "source": [
                "# 3. 반복문(for) 연습 - 구구단\n",
                "print(\"구구단을 외자!\")\n",
                "\n",
                "for i in range(2, 10):  # 2부터 9까지 반복\n",
                "    print(f\"=== {i}단 ===\")\n",
                "    for j in range(1, 10):  # 1부터 9까지 반복\n",
                "        print(f\"{i} x {j} = {i * j}\")\n",
                "    print(\"\")  # 단 사이에 빈 줄 추가"
            ]
        }
    ],
